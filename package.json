{
  "name": "ai-arriendo-pro",
  "version": "3.0.0",
  "description": "Buscador profesional de arriendos con scrapers escalables",
  "main": "dist/server/index.js",
  "scripts": {
    "dev:server": "nodemon --exec ts-node server.ts",
    "client": "cd client && npm start",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "cd client && npm run build",
    "build:production": "npm run build:server && cd client && npm ci && npm run build",
    "start": "node start-server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rimraf dist",
    "setup": "npm install && cd client && npm install",
    "test:scrapers": "ts-node scripts/test-scrapers.ts"
  },
  "keywords": [
    "scraping",
    "real-estate",
    "rental",
    "web-app"
  ],
  "author": "Gonzalo",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.0.0",
    "concurrently": "^8.2.0",
    "jest": "^29.0.0",
    "node-cache": "^5.1.2",
    "nodemon": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@playwright/test": "^1.55.0",
    "@types/cors": "^2.8.19",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "cheerio": "^1.1.2",
    "cli-table3": "^0.6.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "playwright": "^1.55.0",
    "proxy-agent": "^6.3.0",
    "puppeteer": "^24.17.0",
    "rate-limiter-flexible": "^4.0.0",
    "redis": "^4.6.0",
    "socket.io": "^4.7.0",
    "user-agents": "^1.1.0",
    "winston": "^3.11.0"
  }
}
